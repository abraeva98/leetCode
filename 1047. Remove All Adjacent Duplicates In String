Given a string S of lowercase letters, a duplicate removal consists of choosing two adjacent and equal letters, and removing them.

var removeDuplicates = function(s) {
    s = s.split('');
    for (let i = 0; i < s.length; i ++) {
        if (i > 0 && s[i] === s[i - 1]) {
            s.splice(i - 1, 2);
            i = i - 2;
        }
    }
    return s.join('');
};
