
// BFS queue

var allPathsSourceTarget = function(graph) {
    
    let result = [];
    let queue = [[0]];
    let target = graph.length - 1;
    
    while (queue.length !== 0) {
        let path = queue.pop()
        let lastNode = path[path.length - 1];
        
        if (lastNode === target) {
            result.push(path);
        } else {
            let neighbors = graph[lastNode];
            
            for (let neighbor of neighbors) {
                let temp = path.slice();
                temp.push(neighbor);
                queue.push(temp);
            }
        }
    }
    
    
    return result;
};
